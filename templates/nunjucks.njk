gulp.task('nunjucks', function() {
  // Gets .html and .nunjucks files in pages
  return gulp.src('lisamariecrafts/pages/**/*.+(html|nunjucks)')
  // Renders template with nunjucks
  .pipe(nunjucksRender({
      path: ['lisamariecrafts/templates']
    }))
  // output files in app folder
  .pipe(gulp.dest('lisamariecrafts'))
});